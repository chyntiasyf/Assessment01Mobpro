<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">

    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if2125.persegipanjang.ui.hitung.HitungFragment"
        android:label="Luas Persegi Panjang"
        tools:layout="@layout/fragment_hitung" >
        <action
            android:id="@+id/action_hitungFragment_to_rumusFragment2"
            app:destination="@id/rumusFragment2" />
        <action
            android:id="@+id/action_hitungFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_historiFragment"
            app:destination="@id/historiFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_fragmentRuang"
            app:destination="@id/fragmentRuang" />
    </fragment>
    <fragment
        android:id="@+id/rumusFragment2"
        android:name="org.d3if2125.persegipanjang.ui.RumusFragment"
        android:label="Rumus Luas Bangun Datar" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if2125.persegipanjang.ui.AboutFragment"
        android:label="Tentang Aplikasi" />
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if2125.persegipanjang.ui.histori.HistoriFragment"
        android:label="Riwayat"
        tools:layout="@layout/fragment_histori"/>
    <fragment
        android:id="@+id/fragmentRuang"
        android:name="org.d3if2125.persegipanjang.ui.bangunruang.FragmentRuang"
        android:label="Rumus Bangun Ruang"
        tools:layout="@layout/fragment_ruang" />
</navigation>